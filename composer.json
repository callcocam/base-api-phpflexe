{
  "name": "callcocam/project-api-rest-base",
    "description": "A Slim Framework api rest",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/callcocam",
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Campos",
            "email": "callcocam@gmail.com",
            "homepage": "http://www.sigasmart.com.br/"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.5",
        "illuminate/support": "^5.7",
        "illuminate/filesystem": "^5.7",
        "illuminate/database": "^5.7",
        "illuminate/pagination": "^5.7",
        "psr/http-message": "^1.0",
        "firebase/php-jwt": "^5.0",
        "psr/http-server-middleware": "^1.0",
        "symfony/console": "^4.1",
        "rappasoft/laravel-helpers": "^1.0"

    },
    "config": {
        "process-timeout" : 0
    },
    "autoload": {
        "files":[
            "config/helpers.php"
        ],
        "psr-4": {
            "Flexe\\":"v1/src/",
            "App\\":"src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    }

}
